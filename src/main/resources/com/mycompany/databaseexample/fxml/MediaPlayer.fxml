<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.media.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>

<VBox alignment="CENTER" spacing="10" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.databaseexample.MediaPlayerController">
    <children>
        <MediaView fx:id="mediaView" fitHeight="800.0" fitWidth="1200.0" />
        <Button fx:id="skipButton" mnemonicParsing="false" onAction="#skipMedia" prefHeight="25.0" prefWidth="127.0" text="Skip" style="-fx-background-color: #2E2E2E; -fx-text-fill: #D3D3D3; -fx-font-weight: bold; -fx-border-color: #696969; -fx-border-width: 2;" />
    </children>
</VBox>

